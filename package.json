{
  "name": "swdc-vscode",
  "displayName": "Code Time",
  "version": "2.5.4",
  "publisher": "softwaredotcom",
  "description": "Code Time is an open source plugin that provides programming metrics right in Visual Studio Code.",
  "author": {
    "name": "Software.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "icon": "images/app-icon-blue.png",
  "galleryBanner": {
    "color": "#384356",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swdotcom/swdc-vscode"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "code time",
    "time tracker",
    "productivity tracker",
    "project tracker",
    "timer"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "engines": {
    "vscode": "^1.52.0"
  },
  "main": "./dist/extension",
  "scripts": {
    "lint": "tslint -p ./",
    "build": "vsce package --yarn",
    "watch": "tsc -watch -p ./",
    "webpack": "yarn compile:extension & yarn compile:views",
    "webpack-dev": "yarn watch:extension & yarn watch:views",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "vscode:prepublish": "webpack --mode production",
    "test-compile": "tsc -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "codetime.generateProjectSummary",
        "title": "Code Time: View project summary"
      },
      {
        "command": "codetime.softwareKpmDashboard",
        "title": "Code Time: See advanced metrics"
      },
      {
        "command": "codetime.displayReadme",
        "title": "Code Time: Learn more"
      },
      {
        "command": "codetime.disconnectSlackWorkspace",
        "title": "Code Time: Disconnect Slack workspace",
        "icon": {
          "light": "resources/light/remove.svg",
          "dark": "resources/dark/remove.svg"
        }
      },
      {
        "command": "codetime.connectSlackWorkspace",
        "title": "Code Time: Add Slack workspace",
        "icon": {
          "light": "resources/light/add.svg",
          "dark": "resources/dark/add.svg"
        }
      },
      {
        "command": "codetime.viewDashboard",
        "title": "Code Time: View Dashboard"
      },
      {
        "command": "codetime.enableFlow",
        "title": "Code Time: Enable Flow Mode"
      },
      {
        "command": "codetime.pauseFlow",
        "title": "Code Time: Pause Flow Mode"
      }
    ],
    "configuration": [
      {
        "type": "object",
        "title": "Code Time"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-time-sidebar",
          "title": "Code Time",
          "icon": "resources/dark/paw-outlined.svg"
        }
      ]
    },
    "views": {
      "code-time-sidebar": [
        {
          "id": "codetime.webView",
          "type": "webview",
          "name": "",
          "icon": "resources/dark/paw-outlined.svg"
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@types/mocha": "^7.0.2",
    "@types/node": "12.0.4",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/vscode": "^1.35.0",
    "@types/webpack": "^4.41.26",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "loader-utils": "^2.0.0",
    "node-sass": "^5.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^3.5.1",
    "typescript-tslint-plugin": "^1.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "yarn-check-webpack-plugin": "^1.2.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@slack/web-api": "^5.14.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/ws": "^7.4.0",
    "axios": "^0.21.1",
    "clipboardy": "^2.1.0",
    "date-fns": "^2.16.1",
    "file-it": "^1.1.1",
    "jwt-decode": "^2.2.0",
    "moment-timezone": "^0.5.25",
    "node-cache": "^5.1.0",
    "numeral": "^2.0.6",
    "open": "^6.4.0",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "swdc-tracker": "^1.4.4",
    "uuid": "^8.3.1",
    "vsls": "0.3.1291",
    "ws": "^7.4.2"
  }
}
